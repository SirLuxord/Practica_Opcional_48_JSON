package coches;

import java.util.ArrayList;

public class Autos{

	private ArrayList<Auto> auto;
	public Autos(){}
	public Autos(ArrayList<Auto> auto){
		this.auto = auto;
	}
	
	@Override
	public String toString() {
		return "Autos [auto=" + auto + "]";
	}
	/*
