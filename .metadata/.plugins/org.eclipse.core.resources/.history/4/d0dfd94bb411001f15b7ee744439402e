	}
	*/
	public void imprimirAutos() {
		for(int i=0;i<auto.size();i++) {
            auto.get(i).imprimirAuto();
        }
	}
	
	public void aÃ±adirAutos(Auto nuevoauto) {
		this.auto.add(nuevoauto);
	}
}
